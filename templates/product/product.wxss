.list{
  padding: 20rpx;
  display: flex;
  background: #fff;
  margin-top: 20rpx;
}
.img image{
  width: 184rpx;
  height: 184rpx;
  padding-right: 20rpx;
}
.info{
  flex: 1;
  position: relative;
}
.list-desc{
  color: #666;
  font-size: 28rpx;
  line-height: 36rpx;
  /* 超出部分字体隐藏 */
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.list-bottom{
  position: absolute;
  bottom: 0;
  left: 0;
  font-size: 26rpx;
  color: #666;
}
.list-bottom text{
  color: red;
  padding-right: 30rpx;
}